<div>
    {{#each productos}}
        <div>
            <a href="/productos/{{this.nombre}}"></a>
            <br>
            <b>{{this.nombre}}</b>
            <br>
            <b>{{this.stock}}</b>
            <br>
            <img src="{{this.img}}" alt="imagen-producto" width="150px">
            <br>
            <i>{{this.description}}</i>
        </div>
    {{/each}}    
</div>